 @model List<emptyapp.Models.domain.employee>
@{
}

<h1>Employess</h1>

<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Name</th>
			<th>Email</th>
			<th>salary</th>
			<th>Date Of Birth</th>
			<th>Department</th>

		</tr>
	</thead>
	<tbody>
		@foreach(var employee in Model)
		{
			<tr>
				<td>@employee.Id</td>
				<td>@employee.Name</td>
				<td>@employee.Email</td>
				<td>@employee.salary</td>
				<td>@employee.Dateofbirth</td>
				<td>@employee.Department</td>
				 <td><a href="employees/view/@employee.Id">view</a></td>
			</tr>
		}
	</tbody>
</table>